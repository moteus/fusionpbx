<context name="{v_context}"> 
	<extension name="att_xfer" number="att_xfer" continue="false" app_uuid="7bd7a113-0afc-406a-b4a7-33077c22ac39">
		<condition field="destination_number" expression="^att_xfer$" break="never">
			<action application="read" data="2 6 'tone_stream://%(10000,0,350,440)' digits 30000 #"/>
			<action application="set" data="origination_cancel_key=#"/>
			<action application="att_xfer" data="user/${digits}@${domain_name}"/>
			<action application="execute_extension" data="att_xfer_again XML ${context}"/>
		</condition>
		<condition field="destination_number" expression="^att_xfer_again$"/>
		<condition field="${digits}" expression="^\d">
			<action application="execute_extension" data="att_xfer XML ${context}"/>
		</condition>
	</extension>
</context>
